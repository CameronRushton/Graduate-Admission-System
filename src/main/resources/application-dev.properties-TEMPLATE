spring.jpa.database=POSTGRESQL
spring.datasource.platform=postgres
#spring.datasource.url=<your-local-postgresql-url> ex: jdbc:postgresql://localhost:5432/postgres
#spring.datasource.username=<your-postgresql-username>  ex: postgres
#spring.datasource.password=<your-postgresql-password> ex: carleton
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.generate-ddl=true
spring.jpa.properties.hibernate.jdbc.lob.non_contextual_creation=true